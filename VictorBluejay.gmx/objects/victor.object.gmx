    <event eventtype="9" enumb="32">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>{

// victor interact with thing nearby
interacted = false;

if (global.current_item == "nothing") {

    // find a nearby item
    // Look for a gun
    nearest_gun = instance_nearest(x, y, gun_dropped);
    if (instance_exists(nearest_gun)) {
        dist_away = distance_to_point(nearest_gun.x, nearest_gun.y);
        // pick up the gun if it's close enough
        if (dist_away &lt; 15) {
            global.current_item = "gun";
           with(nearest_gun) instance_destroy();
           interacted = true;
        }
    }
    
    // Look for a mirror
    nearest_mirror = instance_nearest(x, y, mirror_dropped);
    if (!interacted &amp;&amp; instance_exists(nearest_mirror) /*nearest_mirror != undefined*/) {
        dist_away = distance_to_point(nearest_mirror.x, nearest_mirror.y);
        // pick up the gun if it's close enough
        if (dist_away &lt; 15) {
            global.current_item = "mirror";
            with(nearest_mirror) instance_destroy();
            interacted = true;
        }
    }

    // find a nearby bush to search
    nearest_bush = instance_nearest(x, y, bush);
    if (!interacted &amp;&amp; nearest_bush != undefined) {
        dist_away = distance_to_point(nearest_bush.x, nearest_bush.y);
     
        if (dist_away &lt; 40) {
            // call the "ruffle" bush event
            if (nearest_bush.ruffle == 0) {
                nearest_bush.ruffle = 1;
                nearest_bush.ruffle_remaining = 5;
            }
            
            // see if there is an item inside of it.
            if (nearest_bush.contents != "nothing") {
                global.current_item = nearest_bush.contents;
                nearest_bush.contents = "nothing";
                // play a leaf rustle sound
                audio_play_sound(rustle, 10, false);
                interacted = true;
            }
        }
    }
    
} else if (global.current_item == "gun") {
    // drop the gun
    global.current_item = "nothing";
    // put the dropped gun on ground
    instance_create(x, y, gun_dropped);
} else if (global.current_item == "mirror") {
   // drop the mirror
    global.current_item = "nothing";
    // put the dropped mirror on ground
    instance_create(x, y, mirror_dropped);
}

}
</string>
